<div class="block09">
  <div class="content">
    <div class="tag"  style="float: right;">
      <%= link_to '+ Crear Grupo', new_musical_group_path, { :class=>"button_1" } %>
    </div>
  </div>
</div>
<div class="heading">
  <h2>Listado de Grupos</h2>
</div>

<table>
  <tr>
    <th>Grupo</th>
    <th>Estilo de Musica</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @musical_groups.each do |musical_group| %>
  <tr>
    <td><%= musical_group.name %></td>
    <td><%= musical_group.music_style.name %></td>
    <td><%= link_to 'Show', musical_group %></td>
    <td><%= link_to 'Edit', edit_musical_group_path(musical_group) %></td>
    <td><%= link_to 'Destroy', musical_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />
